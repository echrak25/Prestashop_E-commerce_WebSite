{% include '@!PrestaShop/Admin/Common/Grid/Blocks/bulk_actions.html.twig' %}
{% if ets_tc_list_views is defined and ets_tc_list_views is not null %}
  <div class="d-inline-block d-inline-block dropdown dropdown-clickable mr-2 ml-2">
    <div id="form_view_selected2">
      <label>View</label>
        <select name="id_view_selected2" id="id_view_selected2" >
            {% for view in ets_tc_list_views %}
                <option data-fields="{{view.fields}}" value="{{view.id_ets_tc_view}}"{%if view.id_ets_tc_view==ets_tc_id_view_selected%} selected="selected"{%endif%}>{{ view.name }}</option>
            {% endfor %}
        </select>
    </div>
  </div>
{%endif%}
{%if link_customer_manager is defined%}
    <div class="d-inline-block">
        <a id="desc-customer-arrange2" class="btn btn-default btn-outline-secondary ml-2" href="{{link_customer_manager}}"> {{ets_tc_custom_column_text}} </a>
    </div>
{%endif%}
{%if ets_expc_link_exportcustomer_manager is defined%}
    <div class="d-inline-block">
        <a id="desc-customer-export" class="btn btn-default btn-outline-secondary ml-2" href="{{ets_expc_link_exportcustomer_manager}}"> {{ets_expc_custom_export_text}} </a>
    </div>
{%endif%}
